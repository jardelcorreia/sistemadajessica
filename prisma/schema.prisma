generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model OperationalRecord {
  id           String    @id @default(cuid())
  timestamp    DateTime  @default(now())
  activityType String
  responsible  String
  location     String
  description  String
  status       String
  photos       Json?
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}
